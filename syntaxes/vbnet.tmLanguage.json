{
	"fileTypes": ["vb"],
	"name": "VB.NET",
	"patterns": [
	  {
		"match": "'[^\n]*",
		"name": "comment.line.singlequote.vb"
	  },
	  {
		"match": "(\\b(If|Then|Else|ElseIf|End If|For|Next|Do|Loop|Select|Case|End Select|Function|Sub|End Sub|End Function|Dim|As|Private|Public|Const|Class|End Class|Module|End Module)\\b)",
		"name": "keyword.control.vb"
	  },
	  {
		"match": "\\b(\\d+)\\b",
		"name": "constant.numeric.vb"
	  },
	  {
		"match": "(\\b(\\w+)\\b)(?=\\s*\\()",
		"name": "variable.other.local.vb"
	  }
	],
	"scopeName": "source.vb"
  }
  